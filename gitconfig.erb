[user]
  name = <%= MY_NAME %>
  email = <%= MY_EMAIL %>
[color]
  status = auto
  diff = auto
  branch = auto
  ui = true
[alias]
  st = status -sb
  co = checkout
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = /usr/local/bin/atom-beta -w
[apply]
  whitespace = nowarn
[github]
  user = <%= GITHUB_USERNAME %>
  token = <%= GITHUB_TOKEN  %>
[push]
  default = tracking
[branch]
  autosetuprebase = always
[pull]
  rebase = preserve
