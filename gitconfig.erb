[user]
  name = <%= MY_NAME %>
  email = <%= MY_EMAIL %>
[color]
  status = auto
  diff = auto
  branch = auto
  ui = true
[alias]
  st = status -sb
  co = checkout
  br = branch
  unstage = reset HEAD --
  last = log -1 HEAD
  sync = "!git fetch origin master:master && git rebase master"
[core]
  excludesfile = <%= ENV['HOME'] %>/.gitignore
  editor = /usr/local/bin/atom-beta -w
  pager = diff-so-fancy | less --tabs=4 -RFX
[apply]
  whitespace = nowarn
[github]
  user = <%= GITHUB_USERNAME %>
  token = <%= GITHUB_TOKEN  %>
[push]
  default = tracking
[branch]
  autosetuprebase = always
[pull]
  rebase = preserve
[status]
  submodulesummary = true
[diff]
	compactionHeuristic = true
